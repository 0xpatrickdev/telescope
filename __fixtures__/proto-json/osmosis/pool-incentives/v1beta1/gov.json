                "nested": {
                  "ReplacePoolIncentivesProposal": {
                    "options": {
                      "(gogoproto.equal)": true,
                      "(gogoproto.goproto_getters)": false,
                      "(gogoproto.goproto_stringer)": false
                    },
                    "fields": {
